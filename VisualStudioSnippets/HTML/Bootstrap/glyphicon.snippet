<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
	xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Bootstrap 3.x glyphicon</Title>
			<Author>Eric Lebetsamer</Author>
			<Description>Glyphicons</Description>
			<Shortcut>bs-glyphicon</Shortcut>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>icon</ID>
					<ToolTip>The name of the icon</ToolTip>
					<Default>info-sign</Default>
				</Literal>
			</Declarations>
			<Code Language="HTML">
				<![CDATA[<span class="glyphicon glyphicon-$icon$"></span>]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>